{
  "name": "dabaago",
  "version": "1.0.0",
  "description": "Premium minimalist chess puzzle web app with blitz and time-limit modes",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
